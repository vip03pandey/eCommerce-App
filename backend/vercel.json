{
    "version": 2,
    "name": "backend",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js",
            "methods": ["OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "https://e-commerce-app-5avv.vercel.app/",
                "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type,Authorization",
                "Access-Control-Allow-Credentials": "true"
            }
        },
        {
            "src": "/api/(.*)",
            "dest": "server.js",
            "headers": {
                "Access-Control-Allow-Origin": "https://e-commerce-app-5avv.vercel.app",
                "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type,Authorization",
                "Access-Control-Allow-Credentials": "true"
            }
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}